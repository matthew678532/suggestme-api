{
  "name": "suggestme-api",
  "version": "1.0.0",
  "description": "This repository contains the API development for the application suggestme.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "mocha ./test/unit/**/*.test.js",
    "test:integration": "mocha ./test/integration/**/*.test.js",
    "testfile": "node ./test/testfile.js",
    "eslint": "eslint --cache --color ./src/**/*.js ./test/**/*.js",
    "copydir": "copyfiles -u 1 ./src/**/*.js ./src/*.js dist"
  },
  "pre-commit": ["npm run eslint"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matthew678532/suggestme-api.git"
  },
  "keywords": [
    "suggestme",
    "api",
    "node"
  ],
  "author": "Matthew L. Birch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matthew678532/suggestme-api/issues"
  },
  "homepage": "https://github.com/matthew678532/suggestme-api#readme",
  "dependencies": {
    "restify": "^6.3.4"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "eslint": "^4.17.0",
    "mocha": "^5.0.0",
    "pre-commit": "^1.2.2"
  }
}
